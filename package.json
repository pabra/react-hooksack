{
  "name": "react-hooksack",
  "version": "1.3.3",
  "description": "A lightweight, fully typed store for react, based entirely on hooks.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "pretest:coverage_publish": "[ ! -x cc-test-reporter ] && ( curl -L https://codeclimate.com/downloads/test-reporter/test-reporter-latest-linux-amd64 > ./cc-test-reporter && chmod +x ./cc-test-reporter && ./cc-test-reporter before-build ) || true",
    "test:coverage": "jest --coverage --config jestconfig.json",
    "test:coverage_publish": "CC_TEST_REPORTER_ID=42e3a64ee207907b6046792e75806574459a65fe54ddcd734ba2bbc1f777700e ./cc-test-reporter after-build -t lcov --debug",
    "prebuild": "[ -d lib ] && rm -rfv lib/*",
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabra/react-hooksack.git"
  },
  "keywords": [
    "react",
    "hooks",
    "store",
    "lightweight",
    "typescript"
  ],
  "author": "Patrick Braune <pabra@netzfruech.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pabra/react-hooksack/issues"
  },
  "homepage": "https://github.com/pabra/react-hooksack#readme",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.17",
    "@types/react-test-renderer": "^16.8.1",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {}
}
