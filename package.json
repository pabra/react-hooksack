{
  "name": "react-hooksack",
  "version": "1.3.7",
  "description": "A lightweight, fully typed store for react, based entirely on hooks.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "pretest:coverage_publish": "[ ! -x cc-test-reporter ] && ( curl -L https://codeclimate.com/downloads/test-reporter/test-reporter-latest-linux-amd64 > ./cc-test-reporter && chmod +x ./cc-test-reporter && ./cc-test-reporter before-build ) || true",
    "test:coverage": "jest --coverage --config jestconfig.json",
    "test:coverage_publish": "CC_TEST_REPORTER_ID=42e3a64ee207907b6046792e75806574459a65fe54ddcd734ba2bbc1f777700e ./cc-test-reporter after-build -t lcov --debug",
    "prebuild": "[ -d lib ] && rm -rfv lib/* || true",
    "build": "tsc",
    "lint": "eslint src/ --ext .js --ext .ts --ext .jsx --ext .tsx",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabra/react-hooksack.git"
  },
  "keywords": [
    "react",
    "hooks",
    "store",
    "lightweight",
    "typescript"
  ],
  "author": "Patrick Braune <pabra@netzfruech.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pabra/react-hooksack/issues"
  },
  "homepage": "https://github.com/pabra/react-hooksack#readme",
  "devDependencies": {
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.32",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "acorn": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.2.7",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {}
}
